<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="UnicastBusConfig" type="NServiceBus.Config.UnicastBusConfig, NServiceBus.Core" />
  </configSections>
  <UnicastBusConfig>
    <MessageEndpointMappings>
      <add Assembly="HashBus.Twitter.CatchUp.Commands" Type="HashBus.Twitter.CatchUp.Commands.StartCatchUp" Endpoint="HashBus.Twitter.CatchUp" />
      <add Assembly="HashBus.Twitter.Analyzer.Commands" Type="HashBus.Twitter.Analyzer.Commands.AnalyzeTweet" Endpoint="HashBus.Twitter.Analyzer" />
      <add Assembly="HashBus.Twitter.Monitor.Events" Type="HashBus.Twitter.Monitor.Events.TweetReceived" Endpoint="HashBus.Twitter.Monitor" />
    </MessageEndpointMappings>
  </UnicastBusConfig>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="NServiceBus.Core" publicKeyToken="9fc386479f8a226c" culture="neutral" />
        <bindingRedirect oldVersion="6.0.0.0" newVersion="5.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Net.Http.Primitives" publicKeyToken="b03f5f7f11d50a3a" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.2.29.0" newVersion="4.2.29.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <appSettings>
    <add key="MaximumNumberOfTweetsPerCatchUp" value="1000" />
    <add key="defaultTransactionTimeout" value="00:10:00" />
    <add key="NServiceBusConnectionString" value="Data Source=.\SqlExpress;Initial Catalog=HashBus.NServiceBus;Integrated Security=True" />
    <add key="EndpointName" value="HashBus.Twitter.CatchUp" />
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/>
  </startup>
</configuration>
